{
  "manifest_version": 2,
  "name": "lichess-ff",
  "version": "0.1",
  "description": "Chess traps hints for Lichess",
  "icons": {
    "48": "icons/lichess_logo.png"
  },
  "background": {
    "scripts": [
      "libs/jquery-3.5.1.min.js",
      "libs/rxjs.umd.min.js",
      "src/utils.js",
      "src/utils2.js",
      "src/background.js"
    ]
  },
  "page_action": {
    "default_icon": "icons/lichess_logo.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "*://localhost/*",
    "webRequest",
    "storage"
  ],
  "options_ui": {
    "page": "src/options/options.html"
  }
}
