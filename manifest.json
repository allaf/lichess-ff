{
  "manifest_version": 2,
  "name": "lichess-ff",
  "version": "0.1",
  "description": "none",
  "background": {
    "scripts": [
      "libs/jquery-3.5.1.min.js",
      "libs/rxjs.umd.min.js",
      "src/background.js"
    ]
  },
  "page_action": {
    "default_icon": "icons/lichess_logo.jpg",
    "browser_style": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "*://localhost/*",
    "webRequest",
    "storage"
  ]
}
